{{# result-list }}

<ul class="gr-c-result-list {{ class }}">

  {{# item }}

  <li class="gr-c-result-list__item">
    <a href="gr-c-result-list__link">
      {{ link-text }}
    </a>
    <div class="gr-c-result-list__details">
      {{ details }}
    </div>
  </li>

  {{/ item }}

</ul>

{{/ result-list }}
